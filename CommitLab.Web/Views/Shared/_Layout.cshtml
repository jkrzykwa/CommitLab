    <![endif]-->



    <link rel="stylesheet" href="//cdn.jsdelivr.net/cal-heatmap/3.3.10/cal-heatmap.css" />
    <script type="text/javascript" src="//d3js.org/d3.v3.min.js"></script>
    <script type="text/javascript" src="//cdn.jsdelivr.net/cal-heatmap/3.3.10/cal-heatmap.min.js"></script>
    <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>
    @*  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css"> *@


    @Styles.Render("~/content/base")
    @Scripts.Render("~/scripts/modernizr-respond")

</head>
<body style="overflow-x:hidden">
    
    <div id="wrapper" style="width:95%; max-width:95%">
        <!-- Navigation -->
        <nav class="navbar navbar-default navbar-static-top" role="navigation" style="margin-bottom: 0">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>

            </div>




            <!-- /.navbar-top-links -->

            <div class="navbar-default sidebar" role="navigation">
                <div class="sidebar-nav navbar-collapse">
                    <ul class="nav" id="side-menu">

                        <li>
                            <a href="~/home/index/" name="indexBar">
                                <button class="btn btn-primary btn-lg" role="button" style="width:236px;text-align:left">
                                    <i class="fa fa-area-chart fa-fw">
                                    </i> Dashboard
                                </button>
                            </a>
                        </li>
                        <li>
                            <a href="~/home/search/" name="searchBar">
                                <button class="btn btn-primary btn-lg" role="button" style="width:236px; text-align:left">
                                    <i class=" fa fa-search fa-fw">
                                    </i> Search
                                </button>
                            </a>
                        </li>
                        <li>
                            <a href="~/home/NuGet/" name="nuGetBar">
                                <button class="btn btn-primary btn-lg" role="button" style="width:236px; text-align:left">
                                    <i class=" fa fa-th-large fa-fw">
                                    </i> NuGet
                                </button>
                            </a>
                        </li>

                    </ul>
                </div>
                <!-- /.sidebar-collapse -->
            </div>

            <!-- /.navbar-header -->
            <div class="navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                    <li>
                        @*<a href="~/home/index/" name="Login" style="margin:0px;padding:0px; margin-right:15px">
                            <button class="btn btn-primary btn-lg" role="button">
                                @RenderSection("userName", true)
                            </button>
                        </a>*@
                                                
                        <a class="navbar-brand" href="~/home/index/">
                            @User.Identity.Name.Substring(@User.Identity.Name.IndexOf("\\") + 1)
                        </a>
                    </li>
                </ul>
            </div>
            <!-- /.navbar-static-side -->
        </nav>
        <div id="page-wrapper">

            <!-- /.row -->
            <div class="row">
                @RenderBody()
            </div>
        </div>

        <!-- /#page-wrapper -->
    </div>
    <!-- /#wrapper -->
    @Scripts.Render("~/scripts/base")
    @RenderSection("scripts", false)

</body>
    </html>

